    "validator": "^13.11.0"
  },
  "engines": {
    "node": ">=10.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "node-sass": "^9.0.0",
    "parcel-bundler": "^1.12.5",
    "prettier": "^3.1.0"
  }
}
